{
    "autoload": {
        "PennyCLI\\": "./"
    },
    "forwards": [
        "package-manager/config.json",
        "site-builder/config.json"
    ],
    "routes": {
        "init": {
            "action": "PennyCLI\\Setup::init"
        },
        "new-site": {
            "action": "PennyCLI\\SiteQuickstart::new_site"
        },
        "new-theme": {
            "action": "PennyCLI\\ThemeQuickstart::new_theme"
        }
    }
}
